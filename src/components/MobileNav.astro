---
import woo from "../lib/woocommerce";
interface Props {
  title: string;
}
interface resData {
  name: string;
  parent: string;
  count: number;
}
const { title } = Astro.props;
const res = await woo.get("/products/categories");
const data: resData[] = res.data ?? null;
---

<div id="nav-mobile" class="nav-mobile navy home">
  <div class="sub-head">
    <div class="container d-flex justify-content-between align-items-center">
      <div id="nav-icon4" class="">
        <span></span>
        <span></span>
        <span></span>
      </div>
      <div class="logo">
        <a href="/">
          <img src="#" alt={title} class="logo-without-sticky" />
          <img src="#" alt={title} class="logo-with-sticky d-none" />
        </a>
      </div>
      <div class="nav" style="left: -100%;">
        <div class="logo">
          <a href="/">
            <img src="//images/logos/red_Logo.png" alt={title} />
          </a>
        </div>
        <nav>
          <ul class="list-nav">
            <!-- <li class="link active">
              <a
                href="javascript:void(0)"
                data-bs-toggle="modal"
                data-bs-target="#authenticationModal"
              >
                <i class="fa-regular fa-user"></i>
                Login
              </a>
            </li> -->
            {
              data &&
                data.map((item) => (
                  <li class="link close-menu-clicked">
                    <a href="/#hotdogs">{item.name}</a>
                  </li>
                ))
            }
            <li class="link">
              <a href="/contact-us">CONTACT US</a>
            </li>
          </ul>
        </nav>
      </div>
      <div class="profile-cart d-flex align-items-center">
        <div class="profile-cart-content">
          <div class="cart">
            <div class="content d-flex align-items-center">
              <span
                ><svg
                  width="24"
